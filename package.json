    "@types/pako": "^1.0.0",
    "@types/pdf": "0.0.31",
    "es6-promise": "^4.2.4",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "ts-loader": "^4.0.1",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-node-externals": "^1.6.0"
  }
